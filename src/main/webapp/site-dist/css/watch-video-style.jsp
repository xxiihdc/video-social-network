<%@ page language="java" contentType="text/css; charset=UTF-8" pageEncoding="UTF-8"%>
.iframe-container {
	position: relative;
	width: 100%;
	padding-bottom: 56.25%;
	height: 0;
	margin-bottom: 0px;
	height: 100%;
}
.iframe-container iframe {
	position: absolute;
	top: 0;
	left: 0px;
	width: 100%;
	height: 100%;
}

.action {
	margin-left: 15px;
	margin-right: 15px;
}

.comment {
	margin-bottom: 5px;
}

.comment-name {
	margin-bottom: 5px;
}

.comment-link {
	color: blue;
	font-weight: bold;
}

.rep-comment-link {
	font-weight: bold;
}

.reply-cmt {
	display: none;
}

.input-repcmt {
	display: none;
}

.rep-comment-link,.comment-link {
	cursor: pointer;
}
#des{
	height: 50px;
	overflow: hidden;
}